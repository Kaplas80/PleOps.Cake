{
    "metadata": [
      {
        "src": [
          {
            "files": [
                "Cake.Frosting.PleOps.Recipe/*.csproj"
            ],
            "src": "../src"
          }
        ],
        "dest": "api",
        "filter": "filter_config.yml",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [ "api/**", "dev/**" ]
        },
        {
          "files": [ "toc.yml", "index.md" ]
        },
        {
          "files": [ "guides/**" ],
          "exclude": [ "**/*.png", "**/*.drawio" ]
        },
        {
          "files": ["CONTRIBUTING.md"],
          "src": "../"
        }
      ],
      "resource": [
        {
          "files": [ "images/**", "guides/**/*.png", "guides/**/*.drawio" ]
        }
      ],
      "dest": "_site",
      "globalMetadata": {
        "_appTitle": "PleOps Cake recipe",
        "_appFooter": "Copyright (c) 2020 Benito Palacios SÃ¡nchez",
        "_appLogoPath": "images/logo_48.png",
        "_appFaviconPath": "images/favicon.png",
        "_enableSearch": true,
        "_enableNewTab": true,
        "_gitContribute": {
            "apiSpecFolder": "docs/apidoc",
            "repo": "https://github.com/pleonex/PleOps.Cake",
            "branch": "main"
        }
      },
      "fileMetadataFiles": [],
      "template": [
        "default",
        "statictoc",
        "default-widescreen"
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
}
